{
  "expo": {
    "name": "Mini POS",
    "slug": "pos",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pos.png",
    "scheme": "pos",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/pos.png",
      "bundleIdentifier": "com.james_marcus.pos",
      "buildNumber": "2",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/pos.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-sqlite",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/pos.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "11201d06-8b4f-4d90-a500-4687e80f99e0"
      }
    },
    "android": {
      "package": "com.james_marcus.pos",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/pos.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 2
    }
  }
}
