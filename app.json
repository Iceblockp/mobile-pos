{
  "expo": {
    "name": "Mini POS",
    "slug": "pos",
    "version": "4.1.0",
    "orientation": "portrait",
    "jsEngine": "hermes",
    "icon": "./assets/images/pos.png",
    "scheme": "pos",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/pos.png",
      "bundleIdentifier": "com.pyaephyonyo.pos",
      "buildNumber": "9",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSBluetoothAlwaysUsageDescription": "This app needs Bluetooth access to print receipts to Bluetooth printers",
        "NSBluetoothPeripheralUsageDescription": "This app needs Bluetooth access to print receipts to Bluetooth printers"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/pos.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-sqlite",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/pos.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "74480d35-2bb3-4772-bf62-dfed94231f6c"
      }
    },
    "android": {
      "package": "com.pyaephyonyo.pos",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/pos.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 9,
      "permissions": [
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    }
  }
}
